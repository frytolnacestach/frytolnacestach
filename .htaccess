RewriteEngine on

RewriteCond %{HTTPS} off

RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

RewriteCond %{HTTP:X-Forwarded-Proto} !https

RewriteCond %{HTTP_USER_AGENT} !(BlackBerry)

RewriteCond %{HTTP_USER_AGENT} !(Windows\sNT\s5\.[12])

RewriteCond %{HTTP_HOST} ^(www\.)?frytolnacestach.cz

Redirect 301 https://www.frytolnacestach.cz/blog-tri-krize.html https://www.frytolnacestach.cz/blog-trikrizovy-vrch.html

ErrorDocument 404 https://www.frytolnacestach.cz/404.html